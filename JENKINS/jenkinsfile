pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                sh'echo"Building new image on docker"' 
            }
        }
        stage('Test') { 
            steps {
                sh'echo"Testing new software"'  
            }
        }
        stage('Deploy') { 
            steps {
                sh'echo"Deploying new software"'  
            }
       stage('Run') { 
            steps {
                sh'echo"Running new software"'  
            }
        }
    }
}
}