pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                sh'echo"Building new software"' 
            }
        }
        stage('Test') { 
            steps {
                sh'echo"Testing new software"'  
            }
        }
        stage('Deploy') { 
            steps {
                sh'echo"Deploying new software"'  
            }
        }
    }
}